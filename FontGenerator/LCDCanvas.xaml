<UserControl x:Class="FontGenerator.LCDCanvas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FontGenerator"
             mc:Ignorable="d" 
             d:DesignHeight="512" d:DesignWidth="512">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="256"/>
            <RowDefinition Height="256"/>
        </Grid.RowDefinitions>
        <UniformGrid x:Name="canvasGrid" Rows="8" Columns="128" Grid.Row="0">


        </UniformGrid>

        <UniformGrid x:Name="mapGrid" Rows="8" Columns="128" Grid.Row="1">


        </UniformGrid>
        
    </Grid>
</UserControl>
